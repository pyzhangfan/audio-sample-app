		<attr name="ipi_animationDuration" format="integer" />
		<attr name="ipi_pageIndicatorColor" format="color" />
		<attr name="ipi_currentPageIndicatorColor" format="color" />
	</declare-styleable>

	<attr name="primaryColor" format="color" />
